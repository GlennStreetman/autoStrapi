{
  "name": "strapi",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "develop": "cp .envDevelop .env && strapi develop",
    "start": "strapi start",
    "build": "NODE_ENV=production strapi build",
